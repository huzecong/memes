				<key>ActionNameComment</key>
				<string>Copy to Clipboard</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>on run {input, parameters}	set chosenMeme to input	-- Copy image to clipboard	set previewRunning to application "Preview" is running	tell application "System Events"		set activeApp to name of the first application process where it is frontmost	end tell	tell application "Preview"		open chosenMeme		activate		tell application "System Events" to Â¬			tell process "Preview"				keystroke "a" using command down				keystroke "c" using command down			end tell	end tell	tell application "Preview" to close front window	if not previewRunning then tell application "Preview" to quit		tell application "System Events" to tell process activeApp to set frontmost to trueend run</string>
				</dict>
